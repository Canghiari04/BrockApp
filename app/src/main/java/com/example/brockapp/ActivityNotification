val inputBuilder = Data.Builder()
        inputBuilder.putString("type", "0")
        inputBuilder.putString("title", "CIAO")
        inputBuilder.putString("text", "CIAOCIAO")
        val input = inputBuilder.build()

val workRequest = OneTimeWorkRequest.Builder(ActivityRecognitionWorker::class.java).apply {
        setInputData(input)
}

WorkManager.getInstance(this).enqueue(workRequest.build())